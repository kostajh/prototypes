mw-core
    nuke; gerrit-sweep.sh; and git checkout -Bupstream-ooui; and nano composer.json; and composer update; and nano maintenance/resources/foreign-resources.yaml; and php maintenance/resources/manageForeignResources.php --conf=includes/DefaultSettings.php verify; sleep 5; nano maintenance/resources/foreign-resources.yaml; and php maintenance/resources/manageForeignResources.php --conf=includes/DefaultSettings.php update ooui; and git add resources/lib/ooui; and php maintenance/resources/manageForeignResources.php --conf=includes/DefaultSettings.php verify; and git add -p; and git commit; and git review

