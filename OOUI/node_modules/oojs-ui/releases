release
    nuke; and git checkout -B release -t origin/master; and npm install; and composer update; and npm test; and composer test; and git log --format='* %s (%aN)' --no-merges --reverse v(node -e 'console.log(require("./package.json").version);')...HEAD | grep -v "Localisation updates from" | sort

    git add -p; and git commit -m "Tag vX.X.X"

    git show
    git review

    nuke; and git checkout origin/master; and git show; and git tag -s v0.XX.X -m 'Signed v0.XX.X release'
    npm run publish-build; and git push --tags; and npm publish

    git log v(git show package.json | grep -E '^- ' | awk '{ split($0,a,"\""); print a[4];}')..v(git show package.json | grep -E '^\+ ' | awk '{ split($0,a,"\""); print a[4];}') | grep Bug: | sort | uniq

